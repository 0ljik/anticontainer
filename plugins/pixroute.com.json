{
	"type": "resolver",
	"ns": "downthemall.net",
	"prefix": "pixroute.com",
	"match": "^http://(?:[\\w\\d]+\\.)?pixroute\\.com/.+/.+\\.html$",
	"finder": "href=\"javascript:.*?src=\"(.+?)\"",
	"useOriginName": true,
	"cleaners": [
		{ "pattern": "\\.html$", "replacement": "" }
	],
	"builder": "{1}",
	"static": true
}
