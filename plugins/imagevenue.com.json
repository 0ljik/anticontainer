{
	"type": "resolver",
	"ns": "downthemall.net",
	"prefix": "imagevenue.com",
	"match": "^http://(?:[\\w\\d]+\\.)?imagevenue\\.com/[\\w\\d_-]+\\.php",
	"finder": "onLoad=\".*?\"[\\s\\r\\n\\t]*SRC=\"(.+?)\"",
	"gone": "^This image does not exist on this server$",
	"builder": "{1}",
	"decode": true,
	"cleaners": [
		{
			"pattern": "(?:_12[23])?_\\d+loc?(\\.[\\w\\d]+)$",
			"replacement": "$1"
		}
	]
}