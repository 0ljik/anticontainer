{
    "type": "expander",
    "ns": "http://www.instagram.com/",
    "author": "MegaScience",
    "prefix": "instagram.com (Image/Video Expander)",
    "match": "^https?://(?:[\\w\\d]+\\.)?(?:instagram\\.com|instagr\\.am)/p/",
    "static": true,
    "finder": "property=\"og:(?:video|image)\" content=\"(.+?)\\.com\/.*?\/((?:[\\d]+_){3}[\\w]\\.[\\w\\d]+)(?:\\?.+?)?\"",
    "generator": "{1}.com/hphotos/{2}"
}
