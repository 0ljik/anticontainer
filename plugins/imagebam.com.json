	"type": "resolver",
	"ns": "downthemall.net",
	"prefix": "imagebam.com",
	"match": "^http://(?:[\\w\\d]+\\.)?imagebam\\.com/image/",
	"finder": "href=(\"|')(.+?/(dl|download).+?)\\1\\s+?class=(?:\"|')buttonblue",
	"gone": "has been deleted|violated our terms|could not be located",
